{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/lib/data.js",
    "src/components/postUser/PostUser.jsx",
    "src/app/store/page.js",
    "src/app/store/[id]/page.js",
    "src/app/iu/books/BookCard.js"
, "src/app/iu/books/AddToCart.js", "src/app/page.jsx"  ],
  "exclude": ["node_modules"]
}
